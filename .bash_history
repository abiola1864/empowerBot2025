def get_active_quizzes(conn):
    query = """
        SELECT DISTINCT q.quiz 
        FROM questions q
        JOIN quiz_status qs ON q.quiz = qs.quiz
        WHERE qs.enabled = 1
    """
    cur = conn.cursor()
    cur.execute(query)
    quizzes = [row[0] for row in cur.fetchall()]
    return quizzes
